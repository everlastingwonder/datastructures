#include <stdio.h>
#include <common.h>

long power(long base, int exp) {
  if(exp == 1) {
    return base;
  } else {
    return (power(base, exp - 1) * base);
  }
}



#include <stdio.h>
#include <common.h>

long power(long base, int exp) {
  int i;
  long ans = 1;

  for(i = 0; i < exp; i++) {
    ans = (ans * base);
  }

  return ans;
}
